CREATE TABLE IF NOT EXISTS DeliveryTable (
    ID SERIAL PRIMARY KEY,
    pda VARCHAR(255),
    rcn VARCHAR(255),
    status VARCHAR(255),
    comments VARCHAR(255),
    startLocation VARCHAR(255),
    endLocation VARCHAR(255),
    edd TIMESTAMP,
    rdd TIMESTAMP,
    eda TIMESTAMP,
    SAMM_Details VARCHAR(255),
    TCN CHAR(17),
    NSN CHAR(13),
    nomenclature VARCHAR(255),
    QTY INT,
    DODIC VARCHAR(255),
    ST FLOAT,
    Depot VARCHAR(255),
    APOE VARCHAR(255),
    Bol CHAR(13),
    SDT_Cost DOUBLE PRECISION,
    Carrier VARCHAR(255),
    ETA_to_APOE TIMESTAMP,
    Delivery_Window TIMESTAMP,
    Truck_status VARCHAR(255),
    SAAM_Status VARCHAR(255)
);
